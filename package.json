    "@reliverse/pathkit": "^1.3.4",
    "@reliverse/reliarg": "^1.0.3",
    "@reliverse/relico": "^1.3.5",
    "@reliverse/relifso": "^1.4.5",
    "@reliverse/relinka": "^1.5.5",
    "@reliverse/runtime": "^1.0.3",
    "@trpc/server": "^11.5.0",
    "ansi-escapes": "^7.0.0",
    "arktype": "^2.1.20",
    "c12": "^3.2.0",
    "cli-spinners": "^3.2.0",
    "commander": "^14.0.0",
    "detect-package-manager": "^3.0.2",
    "effect": "^3.17.9",
    "enquirer": "^2.4.1",
    "figlet": "^1.8.2",
    "gradient-string": "^3.0.0",
    "jiti": "^2.5.1",
    "json-schema": "^0.4.0",
    "log-update": "^6.1.0",
    "node-emoji": "^2.2.0",
    "omelette": "^0.4.17",
    "ora": "^8.2.0",
    "pkg-types": "^2.3.0",
    "prompts": "^2.4.2",
    "sisteransi": "^1.0.5",
    "terminal-kit": "^3.1.2",
    "terminal-size": "^4.0.0",
    "ts-regex-builder": "^1.8.2",
    "tsx": "^4.20.4",
    "unicorn-magic": "^0.3.0",
    "valibot": "^1.1.0",
    "wrap-ansi": "^9.0.0",
    "zod": "^4.0.17",
    "zod-to-json-schema": "^3.24.6"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.2.0",
    "@orpc/contract": "^1.8.3",
