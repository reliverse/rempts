    "ansi-escapes": "^7.0.0",
    "cli-spinners": "^3.2.0",
    "figlet": "^1.8.2",
    "gradient-string": "^3.0.0",
    "log-update": "^6.1.0",
    "ora": "^8.2.0",
    "pkg-types": "^2.3.0",
    "sisteransi": "^1.0.5",
    "terminal-size": "^4.0.0",
    "ts-regex-builder": "^1.8.2",
    "wrap-ansi": "^9.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.2.2",
