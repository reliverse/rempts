{
  "name": "@reliverse/rempts",
  "author": "reliverse",
  "version": "1.7.5",
  "type": "module",
  "license": "MIT",
  "keywords": [
    "reliverse",
    "prompts",
    "rempts",
    "citty",
    "cli",
    "unjs",
    "toolkit",
    "cli-toolkit",
    "cli-prompts"
  ],
  "description": "@reliverse/rempts is a modern, type-safe toolkit for building delightful cli experiences. it's fast, flexible, and made for developer happiness. file-based commands keep things simple.",
  "scripts": {
    "dev": "node -e 'console.log(`ðŸ‘‰ bun dev:[prompts,modern,classic]`)'",
    "dev:prompts": "bun example/prompts/mod.ts",
    "dev:modern": "bun example/launcher/modern.ts",
    "dev:classic": "bun example/launcher/classic.ts",
    "latest": "bun update --latest && bun check",
    "check": "tsc --noEmit && eslint --cache --fix . && biome check --fix --unsafe . && knip",
    "agg": "dler tools --tool agg --input src/libs/core/core-impl --out src/libs/core/core-main.ts --recursive --named --strip src/libs/core",
    "vi": "vitest",
    "pub": "dler pub"
  },
  "dependencies": {
    "@figliolia/chalk-animation": "^1.0.4",
    "@reliverse/reliarg": "^1.0.3",
    "@reliverse/relico": "^1.1.2",
    "@reliverse/relinka": "^1.4.5",
    "@reliverse/runtime": "^1.0.3",
    "ansi-escapes": "^7.0.0",
    "c12": "^3.0.3",
    "cli-spinners": "^3.2.0",
    "detect-package-manager": "^3.0.2",
    "figlet": "^1.8.1",
    "fs-extra": "^11.3.0",
    "gradient-string": "^3.0.0",
    "log-update": "^6.1.0",
    "node-emoji": "^2.2.0",
    "ora": "^8.2.0",
    "pathe": "^2.0.3",
    "pkg-types": "^2.1.0",
    "sisteransi": "^1.0.5",
    "terminal-kit": "^3.1.2",
    "terminal-size": "^4.0.0",
    "ts-regex-builder": "^1.8.2",
    "wrap-ansi": "^9.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@eslint/js": "^9.26.0",
    "@reliverse/dler": "^1.2.3",
    "@reliverse/relidler-cfg": "^1.1.3",
    "@stylistic/eslint-plugin": "^4.2.0",
    "@total-typescript/ts-reset": "^0.6.1",
    "@types/bun": "^1.2.13",
    "@types/figlet": "^1.7.0",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^22.15.18",
    "@types/terminal-kit": "^2.5.7",
    "@types/wrap-ansi": "^8.1.0",
    "eslint": "^9.26.0",
    "eslint-plugin-no-relative-import-paths": "^1.6.1",
    "eslint-plugin-perfectionist": "^4.13.0",
    "jiti": "^2.4.2",
    "knip": "^5.56.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.32.1",
    "vitest": "^3.1.3"
  }
}
