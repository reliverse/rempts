    "@total-typescript/ts-reset": "^0.6.1",
    "@types/bun": "^1.2.20",
    "@types/figlet": "^1.7.0",
    "@types/node": "^24.3.0",
    "@types/omelette": "^0.4.5",
    "@types/prompts": "^2.4.9",
    "@types/terminal-kit": "^2.5.7",
    "@types/wrap-ansi": "^8.1.0",
    "@valibot/to-json-schema": "^1.3.0",
    "expect-type": "^1.2.2",
    "fs-syncer": "^0.5.3",
    "minimatch": "^10.0.3",
    "obuild": "^0.2.1",
    "picocolors": "^1.1.1",
