{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "project": ["**/*.{ts,js}"],
  "entry": [
    "*.{config,cfg}.{ts,js}",
    "src/mod.ts",
    "example/**/*.ts",
    ".config/**/*.ts"
  ],
  "ignore": [
    "node_modules",
    "dist-jsr",
    "dist-npm",
    "dist-libs",
    "src/types.ts",
    ".tests/**/trpc-orpc-cli/**",
    ".tests/figures.test.ts"
  ],
  "includeEntryExports": false,
  "ignoreDependencies": [
    "@orpc/contract",
    "@valibot/to-json-schema",
    "eslint-plugin-codegen",
    "eslint-plugin-mmkal",
    "expect-type",
    "fs-syncer",
    "picocolors",
    "strip-ansi"
  ]
}
